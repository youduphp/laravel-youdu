{
    "name": "youduphp/laravel-youdu",
    "description": "youdu sdk for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "youdu"
    ],
    "homepage": "https://github.com/youduphp/laravel-youdu",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^6.4|^7.0",
        "illuminate/contracts": "^9.0",
        "illuminate/notifications": "^9.0",
        "illuminate/support": "^9.0",
        "youduphp/youdu": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "YouduPhp\\LaravelYoudu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YouduPhp\\LaravelYoudu\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit ./tests/BaseTest.php",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src ./config"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YouduPhp\\LaravelYoudu\\YouduServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
